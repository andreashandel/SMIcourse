  keywords        = {Algorithms; Antigenic Variation; Computational Biology, methods; Disease Transmission, Infectious; Evolution, Molecular; Genetic Fitness; HIV Infections, immunology, transmission; Host-Pathogen Interactions; Humans; Models, Biological; Molecular Epidemiology; Mutation; Viral Load; Virus Diseases, immunology, transmission},
  nlm             = {PMC2759524},
  nlm-id          = {101238922},
  owner           = {NLM},
  pmc             = {PMC2759524},
  pmid            = {19847288},
  pubmodel        = {Print-Electronic},
  pubstatus       = {ppublish},
