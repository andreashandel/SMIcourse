  issue           = {1786},
  keywords        = {Animals; Animals, Wild; Communicable Diseases, epidemiology, microbiology, veterinary; Mammals; Models, Theoretical; Parturition; Population Density; Reproduction; Seasons; Stochastic Processes; birth pulse; critical community size; seasonality; stochastic model; wildlife epidemiology},
  nlm             = {PMC4046395},
