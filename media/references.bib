  issue           = {6},
  keywords        = {Animals; Chemoreceptor Cells, physiology; Electric Stimulation, methods; Electrophysiology; Microelectrodes; Necturus maculosus; Potassium Chloride, pharmacology; Serotonin, pharmacology; Stimulation, Chemical; Synapses, physiology; Synaptic Transmission; Taste Buds, cytology, drug effects, physiology},
  nlm-id          = {8102140},
