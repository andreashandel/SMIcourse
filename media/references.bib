  keywords        = {Animals; Biological Evolution; Humans; Models, Theoretical; Parasites, pathogenicity; Parasitic Diseases, transmission; Virulence},
  nlm-id          = {0373224},
  owner           = {NLM},
  pmid            = {11831655},
  pubmodel        = {Print},
  pubstatus       = {ppublish},
  revised         = {2010-11-18},
  timestamp       = {2017.08.14},
}

@Article{ewald87,
