  journal = {Journal of the Royal Society, Interface},
  keywords = {Adhesiveness,Biological,drug treatment,Genetic Fitness,Hemagglutinins,Human,Humans,immunology,influenza,Influenza,metabolism,Models,Neuraminidase,Orthomyxoviridae,physiology,transmission,vaccines,Virus Attachment,Virus Release,within-host model},
  nlm-id = {101217269},
