  author = {Holzem, Katherine M and Vinnakota, Kalyan C and Ravikumar, Vinod K and Madden, Eli J and Ewald, Gregory A and Dikranian, Krikor and Beard, Daniel A and Efimov, Igor R},
  year = {2016},
