  author = {Cooper, Vaughn S and Reiskind, Michael H and Miller, Jonathan A and Shelton, Kirsten A and Walther, Bruno A and Elkinton, Joseph S and Ewald, Paul W},
  year = {2002},
  month = jun,
  volume = {269},
